<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <bindings>
      <ws2007HttpBinding>
        <binding name="userNameCredentialBinding" >
          <security >
            <message clientCredentialType="UserName"
                      establishSecurityContext="true"/>
          </security>
        </binding>
      </ws2007HttpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="ignoreServiceCertificate">
          <clientCredentials>
            <serviceCertificate>
              <authentication  certificateValidationMode="None"/>
            </serviceCertificate>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <endpoint   name="calculatorservice"
                  address="http://localhost:8888/calculatorservice"
                  contract="Interface1906.ICalculator"
                  binding="ws2007HttpBinding" >

        <!--<identity>
          <certificateReference storeLocation="LocalMachine"
                                storeName ="My"
                               x509FindType="FindBySubjectName"
                               findValue="lhl-PC"/>
        </identity>-->
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>